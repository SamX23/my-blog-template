{
  "name": "nextplate",
  "description": "An opinionated Next.js template for your next plate üçΩÔ∏è",
  "version": "2.0.0-rc1",
  "repository": "https://github.com/grikomsn/nextplate.git",
  "author": "Griko Nibras <git@griko.id>",
  "scripts": {
    "now-build": "run-s \"build:**\"",
    "build": "env-cmd run-s \"build:**\"",
    "build:scripts": "node scripts/build.js",
    "build:web": "next build && next-sitemap",
    "clean": "rimraf \".next\" \"public/!(.gitkeep)\"",
    "develop": "next dev",
    "format": "run-p \"format:**\"",
    "format:index": "prettier --write \"*.{js,json,md}\"",
    "format:src": "prettier --write \"src/**/*.{css,js,ts,tsx}\"",
    "lint": "eslint --cache --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "validate": "run-s lint type-check"
  },
  "dependencies": {
    "@chakra-ui/core": "1.0.0-rc.5",
    "chrome-aws-lambda": "^5.3.1",
    "favicons": "^6.2.0",
    "next": "canary",
    "next-seo": "^4.7.3",
    "nprogress": "^0.2.0",
    "prism-react-renderer": "^1.1.1",
    "puppeteer-core": "^5.3.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.11.0",
    "react-markdown": "^4.3.1"
  },
  "devDependencies": {
    "@types/node": "^14.11.2",
    "@types/nprogress": "^0.2.0",
    "@types/puppeteer": "^3.0.2",
    "@types/puppeteer-core": "^2.0.0",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "copy": "^0.3.2",
    "env-cmd": "^10.1.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "next-sitemap": "^1.2.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "puppeteer": "^5.3.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.3"
  },
  "license": "MIT"
}
